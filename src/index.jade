doctype
html.no-js
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		title
		meta(name='description', content='')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		link(rel='stylesheet', href='./index.min.css')
	body

		h1 React File Size

		table
			thead
				tr
					th version
					th react.js
					th react.js.gz
					th react.min.js
					th react.min.js.gz
			tbody
				each version, i in versions
					tr
						td #{version.version}
						td #{version.size}
						td #{version.sizeGzipped}
						td #{version.minified}
						td #{version.minifiedGzipped}

		hr

		div(id="linechart_material")


		script(src='//cdnjs.cloudflare.com/ajax/libs/lodash.js/4.6.1/lodash.min.js')
		script(src='//www.gstatic.com/charts/loader.js')
		script(src='./index.min.js')

